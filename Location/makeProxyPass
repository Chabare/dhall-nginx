let Upstream = ../Upstream/Spec

in  λ(proxyPass : ./ProxyPass) →
      merge
        { Address =
            λ(t : { address : Text, port : Natural }) →
              "proxy_pass ${t.address}:${Natural/show t.port};"
        , Upstream =
            λ(t : { upstream : Upstream.Type, scheme : Text }) →
              "proxy_pass ${t.scheme}${t.upstream.name};"
        , _None = λ(t : Bool) → ""
        }
        proxyPass
